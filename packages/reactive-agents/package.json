{
  "name": "reactive-agents",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "description": "The composable AI agent framework built on Effect-TS. Type-safe from prompt to production.",
  "keywords": [
    "ai",
    "agents",
    "llm",
    "effect-ts",
    "typescript",
    "ai-agents",
    "autonomous-agents",
    "react-loop",
    "guardrails"
  ],
  "scripts": {
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reactive-agents/reactive-agents-ts.git",
    "directory": "packages/reactive-agents"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": ["src", "README.md", "LICENSE"],
  "dependencies": {
    "effect": "^3.10.0",
    "@reactive-agents/core": "workspace:*",
    "@reactive-agents/runtime": "workspace:*",
    "@reactive-agents/llm-provider": "workspace:*",
    "@reactive-agents/memory": "workspace:*",
    "@reactive-agents/reasoning": "workspace:*",
    "@reactive-agents/tools": "workspace:*",
    "@reactive-agents/guardrails": "workspace:*",
    "@reactive-agents/verification": "workspace:*",
    "@reactive-agents/cost": "workspace:*",
    "@reactive-agents/identity": "workspace:*",
    "@reactive-agents/observability": "workspace:*",
    "@reactive-agents/interaction": "workspace:*",
    "@reactive-agents/orchestration": "workspace:*",
    "@reactive-agents/prompts": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "bun-types": "latest"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./core": {
      "types": "@reactive-agents/core",
      "import": "@reactive-agents/core"
    },
    "./memory": {
      "types": "@reactive-agents/memory",
      "import": "@reactive-agents/memory"
    },
    "./reasoning": {
      "types": "@reactive-agents/reasoning",
      "import": "@reactive-agents/reasoning"
    },
    "./tools": {
      "types": "@reactive-agents/tools",
      "import": "@reactive-agents/tools"
    },
    "./guardrails": {
      "types": "@reactive-agents/guardrails",
      "import": "@reactive-agents/guardrails"
    },
    "./verification": {
      "types": "@reactive-agents/verification",
      "import": "@reactive-agents/verification"
    },
    "./cost": {
      "types": "@reactive-agents/cost",
      "import": "@reactive-agents/cost"
    },
    "./identity": {
      "types": "@reactive-agents/identity",
      "import": "@reactive-agents/identity"
    },
    "./observability": {
      "types": "@reactive-agents/observability",
      "import": "@reactive-agents/observability"
    },
    "./interaction": {
      "types": "@reactive-agents/interaction",
      "import": "@reactive-agents/interaction"
    },
    "./orchestration": {
      "types": "@reactive-agents/orchestration",
      "import": "@reactive-agents/orchestration"
    },
    "./prompts": {
      "types": "@reactive-agents/prompts",
      "import": "@reactive-agents/prompts"
    }
  }
}
